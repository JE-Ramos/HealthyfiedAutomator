    "recipeInstructions": [
      {
        "@type": "HowToStep",
        "text": "Preheat your oven to 375\u00b0F (190\u00b0C)."
      },
      {
        "@type": "HowToStep",
        "text": "Rinse the cauliflower and broccoli under cold water, then chop them into bite-sized florets."
      },
      {
        "@type": "HowToStep",
        "text": "Dice the red, yellow, and green capsicum."
      },
      {
        "@type": "HowToStep",
        "text": "In a large bowl, whisk the eggs and season with salt."
      },
      {
        "@type": "HowToStep",
        "text": "Grease a baking dish with butter, then add the chopped vegetables. Pour the whisked eggs over the vegetables evenly."
      },
      {
        "@type": "HowToStep",
        "text": "Sprinkle the dried thyme leaves over the top."
      },
      {
        "@type": "HowToStep",
        "text": "Bake in the preheated oven for 30-35 minutes, or until the eggs are set and the vegetables are tender.",
