    "recipeInstructions": [
      {
        "@type": "HowToStep",
        "text": "Wash the broccoli under cold water and cut into florets."
      },
      {
        "@type": "HowToStep",
        "text": "Peel and mince the garlic."
      },
      {
        "@type": "HowToStep",
        "text": "In a pan, heat the olive oil over medium heat and saut\u00e9 the minced garlic until fragrant, about 2 minutes."
      },
      {
        "@type": "HowToStep",
        "text": "Add the broccoli florets to the pan and saut\u00e9 for 5 minutes."
      },
      {
        "@type": "HowToStep",
        "text": "Pour the coconut aminos over the broccoli, stir well, and cook for another 5 minutes until the broccoli is tender but still crisp."
      }
    ],
