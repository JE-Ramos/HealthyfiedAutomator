    "recipeInstructions": [
      {
        "@type": "HowToStep",
        "text": "Preheat your oven to 375\u00b0F (190\u00b0C)."
      },
      {
        "@type": "HowToStep",
        "text": "Pat the salmon tail dry with paper towels to remove any excess moisture."
      },
      {
        "@type": "HowToStep",
        "text": "Mix 1 tablespoon of ground black pepper, 1 teaspoon of McCormick Paprika, and 2 tablespoons of fresh lemon juice in a small bowl to create the marinade."
      },
      {
        "@type": "HowToStep",
        "text": "Rub the marinade evenly over both sides of the salmon tail."
      },
      {
        "@type": "HowToStep",
        "text": "Place the salmon tail on a baking sheet lined with parchment paper."
      },
      {
        "@type": "HowToStep",
        "text": "Bake in the preheated oven for 20 minutes, or until the salmon is cooked through and flakes easily with a fork."
      }
    ],
