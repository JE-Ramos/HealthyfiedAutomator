    "recipeInstructions": [
      {
        "@type": "HowToStep",
        "text": "Place the Greek live yogurt into a mixing bowl."
      },
      {
        "@type": "HowToStep",
        "text": "Add the frozen raspberries, flaxseeds, chia seeds, and raw mango pieces to the bowl."
      },
      {
        "@type": "HowToStep",
        "text": "Mix all the ingredients thoroughly until well combined."
      },
      {
