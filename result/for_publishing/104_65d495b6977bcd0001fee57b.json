    "recipeInstructions": [
      {
        "@type": "HowToStep",
        "text": "Rinse the tomatoes under cold water and pat them dry."
      },
      {
        "@type": "HowToStep",
        "text": "Cut the tomatoes into halves."
      },
      {
        "@type": "HowToStep",
        "text": "Preheat your grill to medium-high heat."
      },
      {
        "@type": "HowToStep",
        "text": "Sprinkle the fresh thyme over the cut sides of the tomatoes."
      },
      {
        "@type": "HowToStep",
        "text": "Place the tomatoes cut-side down on the grill and cook for 5 minutes."
      },
      {
        "@type": "HowToStep",
        "text": "Turn the tomatoes over and grill for another 4 minutes until they are soft and the skins are slightly charred."
      }
    ],
