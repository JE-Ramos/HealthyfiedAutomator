    "recipeInstructions": [
      {
        "@type": "HowToStep",
        "text": "Rinse the yellow bell peppers and tomatoes under cold water."
      },
      {
        "@type": "HowToStep",
        "text": "Dice the yellow bell peppers and tomatoes into small pieces."
      },
      {
        "@type": "HowToStep",
        "text": "Finely chop the thyme."
      },
      {
        "@type": "HowToStep",
        "text": "In a large mixing bowl, combine the diced yellow bell peppers, tomatoes, and chopped thyme. Mix gently to combine."
      },
      {
        "@type": "HowToStep",
        "text": "Let the salsa sit for about 10 minutes to allow the flavors to meld together."
      }
