    "recipeInstructions": [
      {
        "@type": "HowToStep",
        "text": "Begin by thinly slicing the tomatoes and red onion."
      },
      {
        "@type": "HowToStep",
        "text": "Peel the daikon radish and slice it thinly."
      },
      {
        "@type": "HowToStep",
        "text": "In a dry pan over medium heat, lightly toast the dried fish until fragrant and slightly crumbly, about 5 minutes."
      },
      {
        "@type": "HowToStep",
        "text": "In a large bowl, combine the sliced tomatoes, red onion, and daikon radish."
      },
      {
        "@type": "HowToStep",
        "text": "Crumble the toasted dried fish over the salad."
      },
      {
        "@type": "HowToStep",
        "text": "Season with ground pepper and toss everything together until well combined."
      }
    ],
