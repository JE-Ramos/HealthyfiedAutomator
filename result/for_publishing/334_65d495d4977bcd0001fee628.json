    "recipeInstructions": [
      {
        "@type": "HowToStep",
        "text": "Begin by juicing the lemon until you have the required amount of lemon juice."
      },
      {
        "@type": "HowToStep",
        "text": "Chop the tomatoes, yellow bell pepper, green capsicum, and red onion into small, bite-sized pieces."
      },
      {
        "@type": "HowToStep",
        "text": "Mince the garlic cloves finely."
      },
      {
        "@type": "HowToStep",
        "text": "In a large mixing bowl, combine the chopped tomatoes, yellow bell pepper, green capsicum, red onion, and minced garlic."
      },
      {
        "@type": "HowToStep",
        "text": "Add the lemon juice and black pepper to the bowl. Toss all ingredients until they are well mixed and evenly coated with the dressing."
      }
