    "recipeInstructions": [
      {
        "@type": "HowToStep",
        "text": "Thinly slice the 200g of red cabbage."
      },
      {
        "@type": "HowToStep",
        "text": "Cut the 57g of orange bell pepper into thin strips."
      },
      {
        "@type": "HowToStep",
        "text": "Grate the 15g of cheddar cheese and set aside."
      },
      {
        "@type": "HowToStep",
        "text": "In a skillet over medium heat, cook the cabbage and bell pepper with the 0.25 cup of tomato puree until the vegetables are tender, about 10 minutes."
      },
      {
        "@type": "HowToStep",
        "text": "Serve hot, garnishing with the grated cheddar cheese on top."
      }
