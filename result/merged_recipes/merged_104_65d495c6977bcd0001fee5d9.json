    "recipeInstructions": [
      {
        "@type": "HowToStep",
        "text": "Trim the woody ends off the asparagus."
      },
      {
        "@type": "HowToStep",
        "text": "Rinse the asparagus under cold water to remove any dirt or sediment."
      },
      {
        "@type": "HowToStep",
        "text": "Bring a pot of water to a boil and blanch the asparagus for 2-3 minutes until vibrant and slightly tender."
      },
      {
        "@type": "HowToStep",
        "text": "Drain the asparagus and immediately plunge into ice water to stop the cooking process."
      },
      {
        "@type": "HowToStep",
        "text": "Pat the asparagus dry with paper towels."
      },
      {
        "@type": "HowToStep",
        "text": "Drizzle the fish oil over the asparagus and toss to evenly coat."
      },
      {
        "@type": "HowToStep",
        "text": "Serve the Omega-3 Infused Asparagus as a nutritious side dish.",
