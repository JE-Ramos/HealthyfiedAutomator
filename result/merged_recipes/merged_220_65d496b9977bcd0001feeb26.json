    "recipeInstructions": [
      {
        "@type": "HowToStep",
        "text": "Measure 1 tablespoon of Chia Seeds."
      },
      {
        "@type": "HowToStep",
        "text": "Measure 0.50 cup (240ml) of Almond Milk - Original."
      },
      {
        "@type": "HowToStep",
        "text": "Wash and dice half a medium apple."
      },
      {
        "@type": "HowToStep",
        "text": "Rinse 15 grams of grapes under cold water and pat dry."
      },
      {
        "@type": "HowToStep",
        "text": "Measure 15 grams of almonds."
      },
      {
        "@type": "HowToStep",
        "text": "In a bowl, combine chia seeds with almond milk. Stir well and let it sit for 5 minutes to allow the chia seeds to start absorbing the liquid."
      },
      {
        "@type": "HowToStep",
        "text": "Add the diced apples, grapes, and almonds to the chia mixture. Stir until all ingredients are well combined.",
        "time": "PT5M"
      },
      {
        "@type": "HowToStep",
        "text": "Cover the bowl with a lid or plastic wrap and refrigerate for at least 2 hours or overnight to let the pudding set."
      }
    ],
