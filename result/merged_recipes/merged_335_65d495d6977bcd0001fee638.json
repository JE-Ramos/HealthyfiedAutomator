    "recipeInstructions": [
      {
        "@type": "HowToStep",
        "text": "Preheat your oven to 220 degrees Celsius."
      },
      {
        "@type": "HowToStep",
        "text": "Slice the steamed zucchini into thin, chip-sized pieces."
      },
      {
        "@type": "HowToStep",
        "text": "In a bowl, toss the zucchini slices with the extra virgin coconut oil until evenly coated."
      },
      {
        "@type": "HowToStep",
        "text": "Arrange the zucchini slices in a single layer on a baking sheet lined with parchment paper."
      },
      {
        "@type": "HowToStep",
        "text": "Bake in the preheated oven for 15 minutes, then flip the chips and bake for another 10 minutes or until crisp and golden brown."
      }
