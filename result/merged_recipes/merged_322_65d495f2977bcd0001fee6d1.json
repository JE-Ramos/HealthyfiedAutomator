    "recipeInstructions": [
      {
        "@type": "HowToStep",
        "text": "Chop the tomatoes, dice the yellow pepper and red onion, and peel and cube the cucumber and mango."
      },
      {
        "@type": "HowToStep",
        "text": "In a large bowl, combine the chopped tomatoes, diced yellow pepper, cubed cucumber, cubed mango, and sliced red onion."
      },
      {
        "@type": "HowToStep",
        "text": "Drizzle the salad with 4 tablespoons of cane vinegar and toss well to coat all the ingredients evenly."
      },
      {
        "@type": "HowToStep",
        "text": "Allow the salad to marinate for at least 15 minutes before serving to enhance the flavors."
      }
