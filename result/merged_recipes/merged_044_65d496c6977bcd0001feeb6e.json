    "recipeYield": "8",
    "recipeIngredient": [
      "1 cup, shredded, Coconut Meat",
      "1 large, White onion",
      "1 tsp, Salt",
      "10 g, lemon grass",
      "12 clove , Garlic",
      "2 tsp, ground, Black pepper",
      "3 cup, Coconut Water",
      "3 small (3\" long), Green onions",
      "50 gram(s), Raw Ginger",
      "600 gram, Whole Chicken"
    ],
