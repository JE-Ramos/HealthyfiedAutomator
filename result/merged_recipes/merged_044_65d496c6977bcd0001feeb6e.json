    "recipeInstructions": [
      {
        "@type": "HowToStep",
        "text": "Prepare your ingredients: finely chop the garlic, slice the green onions, dice the white onion, slice the ginger, and shred the coconut meat."
      },
      {
        "@type": "HowToStep",
        "text": "In a large pot, combine the whole chicken, sliced ginger, chopped garlic, diced white onion, and lemon grass. Pour in the coconut water, and season with salt and black pepper."
      },
      {
        "@type": "HowToStep",
        "text": "Bring the pot to a boil, then reduce the heat to low and simmer for about 45 minutes or until the chicken is fully cooked."
      },
      {
        "@type": "HowToStep",
        "text": "Add the shredded coconut meat and sliced green onions. Simmer for an additional 10 minutes."
      }
    ],
