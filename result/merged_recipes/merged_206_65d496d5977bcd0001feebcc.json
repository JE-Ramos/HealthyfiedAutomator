    "recipeInstructions": [
      {
        "@type": "HowToStep",
        "text": "Peel and cut the sweet potato into cubes."
      },
      {
        "@type": "HowToStep",
        "text": "Dice the red capsicum."
      },
      {
        "@type": "HowToStep",
        "text": "Preheat the oven to 180°C (350°F)."
      },
      {
        "@type": "HowToStep",
        "text": "In a mixing bowl, combine sweet potato cubes, diced red capsicum, and unsalted butter.",
        "time": "PT10M"
      },
      {
        "@type": "HowToStep",
        "text": "Transfer the mixture to a baking tray and bake for 25 minutes or until the sweet potatoes are tender.",
        "time": "PT25M"
      },
      {
        "@type": "HowToStep",
        "text": "Remove from the oven and sprinkle with Parmesan cheese before serving."
