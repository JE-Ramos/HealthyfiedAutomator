    "recipeInstructions": [
      {
        "@type": "HowToStep",
        "text": "Soak cashews in hot water for 20 minutes, then drain."
      },
      {
        "@type": "HowToStep",
        "text": "Rinse shirataki noodles under cold water for 2 minutes then boil for 3 minutes. Drain and set aside to dry."
      },
      {
        "@type": "HowToStep",
        "text": "In a blender, combine the cream cheese fluff, unsweetened vanilla almond milk, pesto sauce, and soaked cashews. Blend until smooth."
      },
      {
        "@type": "HowToStep",
        "text": "Toss the shirataki noodles with the cashew pesto mixture until evenly coated.",
        "time": "5 minutes"
      }
