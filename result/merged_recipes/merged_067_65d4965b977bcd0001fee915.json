    "recipeInstructions": [
      {
        "@type": "HowToStep",
        "text": "Peel the unripe plantains and cut them into chunks."
      },
      {
        "@type": "HowToStep",
        "text": "In a pot, boil the plantain chunks until they become tender, about 20 minutes."
      },
      {
        "@type": "HowToStep",
        "text": "While the plantains are boiling, finely dice the red bell pepper."
      },
      {
        "@type": "HowToStep",
        "text": "Once the plantains are tender, drain the water and transfer the plantains to a large bowl."
      },
      {
        "@type": "HowToStep",
        "text": "Add the evaporated milk, salted butter, salt, and diced red bell pepper to the warm plantains."
      },
      {
        "@type": "HowToStep",
        "text": "Mash the mixture until smooth and well combined."
      }
    ],
