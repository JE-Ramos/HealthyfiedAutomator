    "recipeInstructions": [
      {
        "@type": "HowToStep",
        "text": "Rinse the Garlic Marble Potatoes under cold water until clean."
      },
      {
        "@type": "HowToStep",
        "text": "Boil the potatoes in a pot of salted water until they are fork-tender, about 15 minutes."
      },
      {
        "@type": "HowToStep",
        "text": "Drain the potatoes and let them cool slightly. Once cool enough to handle, cut them in half."
      },
      {
        "@type": "HowToStep",
        "text": "In a mixing bowl, gently toss the halved potatoes with the All Purpose Pesto until evenly coated."
      },
      {
        "@type": "HowToStep",
        "text": "Serve the potatoes warm as a delightful side dish."
      }
    ],
