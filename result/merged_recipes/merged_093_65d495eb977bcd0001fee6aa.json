    "recipeInstructions": [
      {
        "@type": "HowToStep",
        "text": "Prepare the broccoli by rinsing it under cold water, then cut it into florets."
      },
      {
        "@type": "HowToStep",
        "text": "Bring a pot of water to a boil and briefly blanch the broccoli florets for about 2 minutes. Drain and set aside."
      },
      {
        "@type": "HowToStep",
        "text": "In a large pan, heat the extra virgin olive oil over medium heat."
      },
      {
        "@type": "HowToStep",
        "text": "Add the blanched broccoli florets to the pan, season with salt, pepper, and garlic powder. Cook for 5 minutes, stirring occasionally until the broccoli is tender and flavorful."
      }
