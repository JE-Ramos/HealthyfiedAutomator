    "recipeInstructions": [
      {
        "@type": "HowToStep",
        "text": "Preheat your oven to 350°F (175°C)."
      },
      {
        "@type": "HowToStep",
        "text": "Wash the kale and thoroughly dry it with a salad spinner or a clean towel."
      },
      {
        "@type": "HowToStep",
        "text": "Tear the kale leaves into bite-sized pieces, discarding the thick stems."
      },
      {
        "@type": "HowToStep",
        "text": "In a large bowl, massage the kale with olive oil and salt to evenly coat the leaves."
      },
      {
        "@type": "HowToStep",
        "text": "Spread the kale in a single layer on a baking sheet lined with parchment paper."
      },
      {
        "@type": "HowToStep",
        "text": "Bake for 10-15 minutes or until the edges are lightly browned and the kale is crisp. Keep a close eye to prevent burning.",
        "cookTime": "PT15M"
      },
      {
        "@type": "HowToStep",
        "text": "Let the kale chips cool on the sheet for a few minutes before serving to crisp up further."
      }
    ],
