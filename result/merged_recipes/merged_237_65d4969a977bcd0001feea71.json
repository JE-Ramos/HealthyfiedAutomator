    "recipeYield": "12",
    "recipeIngredient": [
      "1 cup(s), Coconut Flour",
      "1 tbl, Extra Virgin Coconut Oil",
      "1 tsp(s), Salt",
      "12 banana, Saba Banana",
      "3 tsp, Baking Powder",
      "5 egg, Eggs"
    ],
    "nutrition": {
