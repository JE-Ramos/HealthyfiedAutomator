    "recipeInstructions": [
      {
        "@type": "HowToStep",
        "text": "Mash the Saba Banana until smooth."
      },
      {
        "@type": "HowToStep",
        "text": "In a large bowl, mix together the mashed banana, coconut flour, eggs, baking powder, and salt until well combined."
      },
      {
        "@type": "HowToStep",
        "text": "Heat a non-stick skillet over medium heat and add a tablespoon of extra virgin coconut oil."
      },
      {
        "@type": "HowToStep",
        "text": "Pour a scoop of the pancake batter into the skillet, cook for about 3 minutes on each side or until golden brown.",
        "time": "PT3M"
      },
      {
        "@type": "HowToStep",
        "text": "Repeat the process with the remaining batter, adding more coconut oil to the skillet as needed."
      }
    ],
