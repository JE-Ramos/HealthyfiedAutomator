    "recipeInstructions": [
      {
        "@type": "HowToStep",
        "text": "In a small bowl, add 0.25 cup of lemon juice and 0.25 cup of vinegar."
      },
      {
        "@type": "HowToStep",
        "text": "Add 0.06 tsp of rock salt and 0.06 tsp(s) of white pepper to the mixture."
      },
      {
        "@type": "HowToStep",
        "text": "Add 1 packet of sugar substitute to the bowl and whisk all the ingredients together until the sugar substitute is fully dissolved and the dressing is well combined."
      },
      {
        "@type": "HowToStep",
        "text": "Taste the dressing and adjust the seasoning if needed."
      },
      {
        "@type": "HowToStep",
        "text": "Store the vinaigrette in a sealed container in the refrigerator until ready to use."
      }
