    "recipeInstructions": [
      {
        "@type": "HowToStep",
        "text": "Thinly slice the Beef Sukiyaki Meat and set aside."
      },
      {
        "@type": "HowToStep",
        "text": "Trim the White Beech Mushrooms and set aside."
      },
      {
        "@type": "HowToStep",
        "text": "In a bowl, mix together Coconut Amino, Salt, ground Pepper, Lemon juice, and Stevia to create a marinade."
      },
      {
        "@type": "HowToStep",
        "text": "Add the sliced Beef Sukiyaki Meat to the marinade and let it marinate for at least 15 minutes."
      },
      {
        "@type": "HowToStep",
        "text": "Heat a pan over medium heat and saut√© the marinated beef for 5 minutes or until browned."
      },
      {
        "@type": "HowToStep",
        "text": "Add the White Beech Mushrooms to the pan and cook for an additional 5 minutes or until the mushrooms are tender."
      },
      {
        "@type": "HowToStep",
        "text": "Serve the Beef Mushroom hot, garnishing with fresh herbs if desired."
      }
