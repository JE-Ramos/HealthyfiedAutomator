    "recipeInstructions": [
      {
        "@type": "HowToStep",
        "text": "Rinse the Adlai Rice under cold running water until the water runs clear."
      },
      {
        "@type": "HowToStep",
        "text": "Cook the Adlai Rice in a pot of boiling water with a pinch of salt. Follow the package instructions for the cooking time, then set aside."
      },
      {
        "@type": "HowToStep",
        "text": "Peel and dice the carrots into small pieces."
      },
      {
        "@type": "HowToStep",
        "text": "Peel and mince the garlic."
      },
      {
        "@type": "HowToStep",
        "text": "In a pan over medium heat, sauté the minced garlic until fragrant, about 2 minutes."
      },
      {
        "@type": "HowToStep",
        "text": "Add the diced carrots to the pan and cook until they are tender, about 5-7 minutes."
      },
      {
        "@type": "HowToStep",
        "text": "Combine the cooked Adlai Rice with the sautéed carrots and garlic. Stir well and season with salt to taste."
      }
    ],
