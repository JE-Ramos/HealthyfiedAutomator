    "recipeInstructions": [
      {
        "@type": "HowToStep",
        "text": "Slice the cucumber into thin rounds."
      },
      {
        "@type": "HowToStep",
        "text": "In a bowl, combine the coconut amino and coconut vinegar with honey, stirring until well mixed."
      },
      {
        "@type": "HowToStep",
        "text": "Add the sliced cucumber to the bowl and gently toss to coat evenly with the dressing."
      },
      {
        "@type": "HowToStep",
        "text": "Let the Cucumber Ensalada marinate for 10 minutes to absorb the flavors."
      },
      {
        "@type": "HowToStep",
        "text": "Serve chilled or at room temperature."
      }
