    "recipeInstructions": [
      {
        "@type": "HowToStep",
        "text": "Rinse the Boneless Milkfish Belly under cold water and pat dry with paper towels."
      },
      {
        "@type": "HowToStep",
        "text": "Finely chop the Garlic and Red Onion, and slice the Tomatoes into small pieces."
      },
      {
        "@type": "HowToStep",
        "text": "Season the Milkfish Belly with ground Pepper."
      },
      {
        "@type": "HowToStep",
        "text": "Heat the Extra-Virgin Olive Oil in a steamer over medium heat."
      },
      {
        "@type": "HowToStep",
        "text": "On the steamer tray, lay out the seasoned Milkfish Belly and top with the chopped Garlic, Onion, and Tomatoes.",
        "time": "Preparation time: 10 minutes"
      },
      {
        "@type": "HowToStep",
        "text": "Cover and steam for about 15 minutes or until the Milkfish Belly is fully cooked.",
        "time": "Cooking time: 15 minutes"
      }
    ],
