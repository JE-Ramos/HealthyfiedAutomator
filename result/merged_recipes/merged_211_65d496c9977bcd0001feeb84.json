    "recipeInstructions": [
      {
        "@type": "HowToStep",
        "text": "Ensure the Sashimi Tuna is fresh and of sushi-grade quality."
      },
      {
        "@type": "HowToStep",
        "text": "Place the tuna in the freezer for about an hour to make slicing easier."
      },
      {
        "@type": "HowToStep",
        "text": "Remove the tuna from the freezer and, using a sharp knife, slice it into thin slices, about 1/8 inch thick."
      },
      {
        "@type": "HowToStep",
        "text": "Arrange the tuna slices neatly on a serving plate."
      },
      {
        "@type": "HowToStep",
        "text": "Optionally, garnish with microgreens or a light drizzle of soy sauce and wasabi on the side for those who enjoy a bit of spice."
      }
