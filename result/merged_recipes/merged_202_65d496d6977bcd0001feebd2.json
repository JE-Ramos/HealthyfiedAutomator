    "recipeInstructions": [
      {
        "@type": "HowToStep",
        "text": "Finely dice the red onion, capsicum (green and red), and set aside."
      },
      {
        "@type": "HowToStep",
        "text": "Heat the extra virgin olive oil in a large skillet over medium heat."
      },
      {
        "@type": "HowToStep",
        "text": "Add the diced onions and capsicum to the skillet and saut√© until they are soft, about 5 minutes."
      },
      {
        "@type": "HowToStep",
        "text": "Increase the heat to medium-high and add the ground beef to the skillet, cooking until browned, about 8 minutes."
      },
      {
        "@type": "HowToStep",
        "text": "Mix in the tomato puree, salt, ground pepper, thyme, basil, and marjoram. Reduce heat to low and let simmer for 15 minutes to allow flavors to blend."
      }
