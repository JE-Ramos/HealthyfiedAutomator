    "recipeInstructions": [
      {
        "@type": "HowToStep",
        "text": "Finely dice the white onion."
      },
      {
        "@type": "HowToStep",
        "text": "Shred the cooked chicken breast into small pieces."
      },
      {
        "@type": "HowToStep",
        "text": "In a bowl, combine the diced onion, shredded chicken breast, organic coconut aminos, ground black pepper, salt, and mayonnaise."
      },
      {
        "@type": "HowToStep",
        "text": "Mix all the ingredients until well combined and the chicken is evenly coated with the mayonnaise and seasoning mixture."
      },
      {
        "@type": "HowToStep",
        "text": "Let the mixture rest in the refrigerator for about 15 minutes to allow the flavors to meld together."
      }
    ],
