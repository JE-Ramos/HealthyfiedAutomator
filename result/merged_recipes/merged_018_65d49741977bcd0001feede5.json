    "recipeIngredient": [
      "0.12 teaspoon, Salt",
      "0.50 tbsp(s), Organic Coconut Aminos",
      "1 tsp, Ground Black Pepper",
      "250 gram, Chicken breast, cooked, skinless",
      "28 gram, White onion",
      "35 g, Mayonnaise"
    ],
