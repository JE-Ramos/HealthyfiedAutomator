    "recipeInstructions": [
      {
        "@type": "HowToStep",
        "text": "Rinse the broccoli under cold water and chop into small florets."
      },
      {
        "@type": "HowToStep",
        "text": "Peel the carrots and chop them finely."
      },
      {
        "@type": "HowToStep",
        "text": "Mince the garlic and finely dice the red onion."
      },
      {
        "@type": "HowToStep",
        "text": "In a skillet over medium heat, cook the ground beef until browned, about 5-7 minutes."
      },
      {
        "@type": "HowToStep",
        "text": "Add the minced garlic, diced red onion, marjoram, salt, and pepper to the beef. Stir and cook for an additional 2-3 minutes."
      },
      {
        "@type": "HowToStep",
        "text": "Steam the broccoli and carrots until tender, about 5 minutes."
      },
      {
        "@type": "HowToStep",
        "text": "Combine the steamed vegetables with the beef mixture. Toss well to combine."
      }
    ],
