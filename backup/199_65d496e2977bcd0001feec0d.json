    "recipeIngredient": [
      "1 medium, Carrots, baby, raw",
      "1 large, Egg, whole, cooked, hard-boiled",
      "2 small whole, Small",
      "350 g, Cucumber",
      "0.50 cup, Mango",
      "3 stick, Imitation crab",
      "10 g, Whole Almonds (Almendras)"
    ],
