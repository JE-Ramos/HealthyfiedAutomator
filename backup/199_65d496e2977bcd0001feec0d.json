    "recipeInstructions": [
      {
        "@type": "HowToStep",
        "text": "Peel and thinly slice the cucumber."
      },
      {
        "@type": "HowToStep",
        "text": "Peel the mango and cut it into small cubes."
      },
      {
        "@type": "HowToStep",
        "text": "Julienne the baby carrots."
      },
      {
        "@type": "HowToStep",
        "text": "Chop the hard-boiled egg into small pieces."
      },
      {
        "@type": "HowToStep",
        "text": "Cut the imitation crab into bite-size pieces."
      },
      {
        "@type": "HowToStep",
        "text": "Chop the almonds coarsely."
      },
      {
        "@type": "HowToStep",
        "text": "In a large mixing bowl, combine the sliced cucumber, cubed mango, julienned carrots, chopped egg, imitation crab, and almonds."
      },
      {
        "@type": "HowToStep",
        "text": "Toss the ingredients gently to ensure they are well mixed. Serve chilled or at room temperature."
      }
