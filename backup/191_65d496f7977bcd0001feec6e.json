    "recipeInstructions": [
      {
        "@type": "HowToStep",
        "text": "Preheat the oven to 375\u00b0F (190\u00b0C)."
      },
      {
        "@type": "HowToStep",
        "text": "Crack the eggs into a bowl and whisk until fully beaten."
      },
      {
        "@type": "HowToStep",
        "text": "Grate the Sharp Cheddar Cheese."
      },
      {
        "@type": "HowToStep",
        "text": "Season the Pangasius fillet with salt and pepper to taste."
      },
      {
        "@type": "HowToStep",
        "text": "Chop the fresh basil leaves finely."
      },
      {
        "@type": "HowToStep",
        "text": "Place the Pangasius fillets in a baking dish."
      },
      {
        "@type": "HowToStep",
        "text": "Pour the beaten eggs over the fillets, ensuring they are well covered."
      },
      {
        "@type": "HowToStep",
        "text": "Sprinkle the grated cheddar cheese on top of the egg-covered fillets."
      },
      {
        "@type": "HowToStep",
        "text": "Bake in the preheated oven for about 20 minutes, or until the fish is cooked through and the cheese is golden brown."
      },
      {
        "@type": "HowToStep",
        "text": "Garnish with freshly chopped basil leaves before serving."
      }
    ],
