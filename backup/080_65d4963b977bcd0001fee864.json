    "recipeInstructions": [
      {
        "@type": "HowToStep",
        "text": "Thinly slice the 60g of white onion."
      },
      {
        "@type": "HowToStep",
        "text": "Season the 300 grams of lean beef evenly with 0.25 teaspoon of salt, 0.25 teaspoon of ground pepper, and 1 teaspoon of Spice World."
      },
      {
        "@type": "HowToStep",
        "text": "Let the seasoned beef sit for 10 minutes to absorb the flavors."
      },
      {
        "@type": "HowToStep",
        "text": "Preheat a grill or skillet over medium-high heat.",
        "image": "Cooking time: 5 minutes"
      },
      {
        "@type": "HowToStep",
        "text": "Cook the beef for 4-5 minutes on each side for medium-rare, or until desired doneness.",
        "image": "Cooking time: 8-10 minutes"
      },
      {
        "@type": "HowToStep",
        "text": "Remove the beef from the heat and let it rest for 5 minutes.",
        "image": "Cooking time: 5 minutes"
      },
      {
        "@type": "HowToStep",
        "text": "Serve hot, garnished with the sliced white onions."
      }
    ],
