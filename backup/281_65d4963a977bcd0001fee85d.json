    "recipeInstructions": [
      {
        "@type": "HowToStep",
        "text": "Season the chicken breast with salt and white pepper."
      },
      {
        "@type": "HowToStep",
        "text": "Grill the chicken breast over medium heat until fully cooked, approximately 10 minutes."
      },
      {
        "@type": "HowToStep",
        "text": "Rinse the Ice Berg Lettuce under cold water and chop it."
      },
      {
        "@type": "HowToStep",
        "text": "Boil the tomato for about 5 minutes, then allow to cool before chopping."
      },
      {
        "@type": "HowToStep",
        "text": "In a large bowl, combine the grilled chicken, chopped lettuce, and tomato."
      },
      {
        "@type": "HowToStep",
        "text": "Drizzle with balsamic vinaigrette and toss to combine."
      },
      {
        "@type": "HowToStep",
        "text": "Sprinkle the All-Bran Regular on top of the salad for an added crunch."
      }
    ],
