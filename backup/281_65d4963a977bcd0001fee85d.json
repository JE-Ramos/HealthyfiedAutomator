    "recipeYield": "1",
    "recipeIngredient": [
      "0.67 dash, Salt, table",
      "0.50 cup  (72gm), Ice Berg Lettuce",
      "1.00 oz, Chicken Breast (Grilled)",
      "0.12 cup, Tomato Boiled",
      "1 tbsp, Spices, pepper, white",
      "1 tsp(s), Balsamic Vinegarette",
      "7.50 g, All-Bran Regular"
    ],
    "nutrition": {
      "@type": "NutritionInformation",
      "calories": "123",
