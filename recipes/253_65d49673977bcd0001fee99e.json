    "recipeInstructions": [
      {
        "@type": "HowToStep",
        "text": "Season the Sukiyaki beef with salt and pepper."
      },
      {
        "@type": "HowToStep",
        "text": "In a large pan, heat the sesame oil over medium heat."
      },
      {
        "@type": "HowToStep",
        "text": "Add the seasoned beef to the pan and cook until browned, about 3-4 minutes per side.",
        "time": "PT8M"
      },
      {
        "@type": "HowToStep",
        "text": "Squeeze the lemon juice evenly over the cooked beef."
      },
      {
        "@type": "HowToStep",
        "text": "Drizzle the teryaki sauce over the beef and cook for an additional 2 minutes to combine the flavors.",
        "time": "PT2M"
      }
