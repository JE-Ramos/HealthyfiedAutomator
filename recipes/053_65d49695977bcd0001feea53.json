    "name": "AvoCoMangoBan Smoothie",
    "recipeYield": "3",
    "recipeIngredient": [
      "1 medium, Banana",
      "1 cup (8 fl oz), Water",
      "0.50 fruit, Avocado",
      "1 cup, shredded, Coconut Meat",
      "2 tbsp, Whipped Cream",
      "1 packet, Stevia In the Raw",
      "0.50 mango, Mango"
    ],
    "nutrition": {
      "@type": "NutritionInformation",
      "calories": "196",
      "carbohydrateContent": "21",
      "fiberContent": "6",
      "sugarContent": "12",
      "sodiumContent": "11",
      "proteinContent": "2",
      "fatContent": "13",
      "saturatedFatContent": "9",
      "monounsaturatedFatContent": "3",
      "polyunsaturatedFatContent": "1",
      "unsaturatedFatContent": 4,
      "transFatContent": "0"
    },
    "recipeInstructions": [
      {
        "@type": "HowToStep",
        "text": "Peel and slice the banana."
      },
      {
        "@type": "HowToStep",
        "text": "Scoop out the avocado flesh and chop the mango into chunks."
      },
      {
        "@type": "HowToStep",
        "text": "Add banana slices, avocado flesh, mango chunks, shredded coconut meat, water, whipped cream, and Stevia to a blender."
      },
      {
        "@type": "HowToStep",
        "text": "Blend on high speed until smooth, about 2 minutes."
      },
      {
        "@type": "HowToStep",
        "text": "Pour into glasses and serve immediately."
      }
    ],
